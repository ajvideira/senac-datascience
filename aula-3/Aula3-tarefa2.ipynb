    "import pandas as pd\n",
    "import json\n",
    "\n",
    "with open('../input/vendas_loja_aninhado.json', 'r', encoding=\"utf-8\") as json_file:\n",
    "    data = json.load(json_file)\n",
    "\n",
    "df_vendas = pd.json_normalize(data, record_path=['vendas'], meta=['loja_id', 'cidade'])\n",
    "\n",
    "display(df_vendas)"
